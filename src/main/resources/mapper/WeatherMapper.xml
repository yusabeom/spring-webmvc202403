<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.mvc.chap05.mapper.WeatherMapper">

    <select id="getCoord" resultType="map">
        SELECT
            nx, ny
        FROM test_location
        WHERE area1 = #{area1}
        AND area2 LIKE CONCAT('%', #{area2}, '%')
        LIMIT 0, 1
    </select>

</mapper>